                                            <dialog id="image-fullscreen">
                                                <div class="modal-header large">
                                                    <div class="close-img-button" aria-label="close">&#215;</div>
                                                </div>
                                                <div class="modal-content no-padding">
                                                    <div class="img-plus no-stretch sharp-corners aspectratio">
                                                        <picture>
                                                            <img id="image-fullscreen-url">
                                                        </picture>
                                                        <div class="img-credit" id="image-fullscreen-credit"></div>
                                                    </div>
                                                </div>
                                            </dialog>


                                            <script type="text/javascript">
                                                const imgModal = document.querySelector('#image-fullscreen');
                                                const openImgModal = document.querySelectorAll('.open-img-modal');
                                                const closeImgModal = document.querySelector('.close-img-button');
                                                const dialogImgElem = document.querySelector('#image-fullscreen-url');
                                                const dialogImgElemCredit = document.querySelector('#image-fullscreen-credit');
                                                const iBody = document.querySelector('body');

                                                openImgModal.forEach((targetOpenModal) => {
                                                    targetOpenModal.addEventListener('click', () => {
                                                        dialogImgElem.src = targetOpenModal.dataset.imgUrl;
                                                        dialogImgElemCredit.innerHTML = targetOpenModal.dataset.imgCredit;
                                                        imgModal.showModal();
                                                        // Disable scroll
                                                        iBody.style.overflow = 'hidden';
                                                    });
                                                });

                                                closeImgModal.addEventListener('click', () => {
                                                    imgModal.close();
                                                    // Enable scroll
                                                    iBody.style.overflow = 'auto';
                                                });
                                            </script>

                                            <!-- carousels is only for the venue logo ticker tape on homepage -->
                                            {% if carousels %}
                                                <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
            <script>
                var flktyWrapAroundAuto = document.getElementsByClassName("flick-carousel--wraparound-autoplay")[0];
                if (flktyWrapAroundAuto) {
                    var flkty_wrap = new Flickity(flktyWrapAroundAuto, {
                        wrapAround: true,
                        autoPlay: 2500,
                        freeScroll: true,
                        prevNextButtons: false,
                        pageDots: false
                    });
                    // This avoids FOUC -- flash of unstyled content
                    flktyWrapAroundAuto.style.opacity = 1.0;
                    flkty_wrap.on('pointerUp', function (event, pointer) {
                        flkty_wrap.playPlayer();
                    });
                }

                var review_content_flkty = document.getElementById("review-content-flkty");
                var review_img_flkty = document.getElementById("review-img-flkty");
                // var review_nav_buttons = document.getElementById("review-nav-buttons");
                var view_all_button = document.getElementById("view-all-button");

                if (review_content_flkty && review_img_flkty) {
                    var flkty__review_img = new Flickity('.review-img--carousel', {
                        wrapAround: true,
                        prevNextButtons: false,
                        pageDots: false,
                        draggable: false,
                        asNavFor: '.review-content--carousel'
                    });
                    var flkty__review_content = new Flickity('.review-content--carousel', {
                        wrapAround: true,
                        prevNextButtons: true,
                        pageDots: true,
                        draggable: true
                    });
                    // This avoids FOUC -- flash of unstyled content
                    // Turn on opacity after intialising flickity sliders
                    review_content_flkty.style.opacity = 1.0;
                    review_img_flkty.style.opacity = 1.0;
                    // review_nav_buttons.style.opacity = 1.0;
                    if (view_all_button) 
                        view_all_button.style.opacity = 1.0;
                    
                    var previousButton = document.querySelector('.button--previous');
                    if (previousButton) {
                        previousButton.addEventListener('click', function () {
                            flkty__review_content.previous();
                            flkty__review_img.previous();
                        });
                    }

                    var nextButton = document.querySelector('.button--next');
                    if (nextButton) {
                        nextButton.addEventListener('click', function () {
                            flkty__review_content.next();
                            flkty__review_img.next();
                        });
                    }
                }

                // Show only review content flickity slider for mobile
                var review_content_flkty_mobile = document.getElementById("review-content-flkty-mobile");
                if (review_content_flkty_mobile) {
                    var flkty__review_content = new Flickity('.review-content--carousel-mobile', {
                        wrapAround: true,
                        prevNextButtons: false,
                        pageDots: true,
                        draggable: true
                    });
                    // This avoids FOUC -- flash of unstyled content
                    // Turn on opacity after intialising flickity sliders
                    review_content_flkty_mobile.style.opacity = 1.0;
                    // review_nav_buttons.style.opacity = 1.0;
                    if (view_all_button) 
                        view_all_button.style.opacity = 1.0;
                    
                    var previousButton = document.querySelector('.button--previous');
                    if (previousButton) {
                        previousButton.addEventListener('click', function () {
                            flkty__review_content.previous();
                        });
                    }

                    var nextButton = document.querySelector('.button--next');
                    if (nextButton) {
                        nextButton.addEventListener('click', function () {
                            flkty__review_content.next();
                        });
                    }
                }
            </script>
        {% endif %}

